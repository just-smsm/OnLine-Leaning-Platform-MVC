@model OnlineLearningPlatform.Models.StudentQuizAttempt

@{
    ViewData["Title"] = "Passed";
}

<div class="container mt-5">
    <div
        class="alert alert-success"
        role="alert"
    >
        <h4 class="alert-heading">Congratulations!</h4>
        <p>You have successfully passed the quiz.</p>
        <hr>
        <p class="mb-0">Your score: <strong>@Model.ScoreAchieved</strong></p>
        <p>Quiz ID: <strong>@Model.QuizId</strong></p>
        <p>Date and Time: <strong>@Model.AttemptDatetime</strong></p>
    </div>
    <a
        href="@Url.Action("Index", "Home")"
        class="btn btn-primary"
    >Back to Home</a>
</div>

